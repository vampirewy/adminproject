{
  "name": "newadmin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:dv2": "vue-cli-service serve --mode dv2",
    "serve:newdv2": "vue-cli-service serve --mode newdv2",
    "serve:alpha": "vue-cli-service serve --mode alpha",
    "alpha": "vue-cli-service build --mode alpha && cross-env NODE_ENV=alpha node ./service/uploadFtp.js",
    "dv2": "vue-cli-service build --mode dv2 && cross-env NODE_ENV=dv2 node ./service/uploadFtp.js",
    "newdv2": "vue-cli-service build --mode newdv2 && cross-env NODE_ENV=newdv2 node ./service/uploadFtp.js",
    "pro": "vue-cli-service build && cross-env NODE_ENV=production node ./service/uploadFtp.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "element-ui": "^2.6.1",
    "file-saver": "^2.0.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-e2e-cypress": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "archiver": "^3.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs": "0.0.1-security",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.2",
    "ora": "^3.4.0",
    "sass-loader": "^7.0.3",
    "scp2": "^0.5.0",
    "ssh2": "^0.8.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
